<center>

<input type="text" id="English"></td> <br><br>
<button type="button" onclick="translateToUbbiDubbi()" > Translate to Ubbi Dubbi </button> <br>
</center>
<br>


<center>
<p id="ubbidubbi"><p>
<script>

function translateToUbbiDubbi() {
var str = document.getElementById("English").value;
var text="";
  for (var i=0; i < str.length; i++){
    if (str.charAt(i) == "a" || str.charAt(i) == "e" || str.charAt(i) == "i" || str.charAt(i) == "o" || str.charAt(i) == "u"){
        text= text+"ub"+ str.charAt(i);
        }
    else{
      text = text+ str.charAt(i)
    }
    }
    document.getElementById("ubbidubbi").innerHTML=text;
    }

</script>
</center>
